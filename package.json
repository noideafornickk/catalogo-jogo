{
  "name": "gamebox",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.6.2",
  "scripts": {
    "dev:web": "pnpm --filter @gamebox/web dev",
    "dev:api": "pnpm --filter @gamebox/api dev",
    "dev": "concurrently \"pnpm dev:api\" \"pnpm dev:web\"",
    "build": "pnpm --filter @gamebox/api build && pnpm --filter @gamebox/web build"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
